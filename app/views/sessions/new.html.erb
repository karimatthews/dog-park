<div class="contents">

  <h1>Login</h1>

  <div class='form' style = 'margin-left:0px;'>
    <%= form_tag '/login' do %>
      <%= label_tag :email, 'Email:' %>
      <%= text_field_tag :email %>
      <br><br>
      <%= label_tag :password, 'Password:' %>
      <%= password_field_tag :password %>
      <br><br>
      <%= submit_tag "Submit" %>
    <% end %>
  </div>
</div>
